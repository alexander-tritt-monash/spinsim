	pages = {75--86},
	annote = {LLVM reference},
	file = {IEEE Xplore Full Text PDF:D\:\\Zotero\\storage\\GPHLGQ2R\\Lattner and Adve - 2004 - LLVM a compilation framework for lifelong program.pdf:application/pdf;IEEE Xplore Abstract Record:D\:\\Zotero\\storage\\26P8MATB\\1281665.html:text/html},
