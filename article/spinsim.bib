	note = {Publisher: American Association for the Advancement of Science
Section: Reports},
	pages = {837--840},
	file = {Snapshot:D\:\\Zotero\\storage\\YKM845QX\\837.html:text/html;Full Text PDF:D\:\\Zotero\\storage\\E8FSQNAW\\Boss et al. - 2017 - Quantum sensing with arbitrary frequency resolutio.pdf:application/pdf},
