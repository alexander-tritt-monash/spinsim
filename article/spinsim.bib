@article{helland_reconstruction_2021,
	title = {On reconstruction of the {Hilbert} space from conceptual variables},
	url = {http://arxiv.org/abs/2108.12168},
	abstract = {In [1] the general problem of deriving the Hilbert space formulation in quantum theory is discussed from the point of view of conceptual variables. A conceptual variable is any variable defined by a person or by a group of persons. These variables may be inaccessible, i.e., impossible to assign numerical value to either by experiments or by other means, or accessible. As one basic assumption, a group K of actions on a space of an inaccessible variable is defined, and accessible functions of this variable are introduced. When such a function is permissible, a term that is precisely defined here, a group of actions G can be introduced on the range of the function. In general in this paper only the existence of the group G is assumed. By using group representation theory on the groups defined on a class of accessible variables, basic Hilbert space formalism is restored under the assumption that the observator or the set of observators has two maximally accessible variables in his (their) mind(s). Operators are connected to each variable, and in the discrete case the possible values of the variables are given by the eigenvalues of the operators. The entire approach may be connected to a general epistemic interpretation of quantum theory. In this paper the main results from [1] are made more precise and more general. It turns out that the conditions of the main theorem there can be weakened in two essential ways: 1) No measurements need to be assumed, so the result is also applicable to general decision situations; 2) States can have arbitrary phase factors. Some consequences of the main result are also briefly discussed here.},
	urldate = {2021-09-13},
	journal = {arXiv:2108.12168 [quant-ph]},
	author = {Helland, Inge S.},
	month = aug,
	year = {2021},
	note = {arXiv: 2108.12168},
	keywords = {Quantum Physics},
	annote = {Comment: 8 pages},
	file = {arXiv Fulltext PDF:D\:\\Zotero\\storage\\K999Z4E3\\Helland - 2021 - On reconstruction of the Hilbert space from concep.pdf:application/pdf;arXiv.org Snapshot:D\:\\Zotero\\storage\\3RXDGXKL\\2108.html:text/html},
}

