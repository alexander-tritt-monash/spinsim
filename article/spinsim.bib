	keywords = {Quantum Physics, Condensed Matter - Disordered Systems and Neural Networks},
	annote = {Comment: 11 pages, 8 figures},
	file = {arXiv Fulltext PDF:D\:\\Zotero\\storage\\JZFKY492\\Braccia et al. - 2021 - Quantum Noise Sensing by generating Fake Noise.pdf:application/pdf;arXiv.org Snapshot:D\:\\Zotero\\storage\\CZNAJN5L\\2107.html:text/html},
}

@article{groszkowski_scqubits_2021,
