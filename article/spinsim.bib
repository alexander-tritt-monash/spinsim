@article{fadel_time-energy_2021,
	title = {The time-energy uncertainty relation for quantum events},
	url = {http://arxiv.org/abs/2108.13974},
	abstract = {Textbook quantum mechanics treats time as a classical parameter, and not as a quantum observable with an associated Hermitian operator. For this reason, to make sense of usual time-energy uncertainty relations such as \${\textbackslash}Delta \{t\}{\textbackslash}Delta E{\textbackslash}gtrsim{\textbackslash}hbar\$, the term \${\textbackslash}Delta t\$ must be interpreted as a time interval, and not as a time measurement uncertainty due to quantum noise. However, quantum clocks allow for a measurement of the "time at which an event happens" by conditioning the system's evolution on an additional quantum degree of freedom. Within this framework, we derive here two \{{\textbackslash}em true\} uncertainty relations that relate the uncertainty in the quantum measurement of the time at which a quantum event happens on a system to its energy uncertainty.},
	urldate = {2021-09-13},
